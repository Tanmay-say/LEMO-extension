{
  "manifest_version": 3,
  "name": "Lemo Extension",
  "version": "1.0.0",
  "description": "A simple Chrome extension for EthOnline hackathon",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Lemo Extension"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}